{
	"name": "swarm-account-manager",
	"displayName": "SwarmAccountManager",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.35.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.swarm-debugging.open"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [{
			"command": "extension.swarm-debugging.open",
			"title": "Open Swarm Debugging Account manager"
		}],
		"menus": {
			"scm/title": [
			]
		},
		"viewsContainers": {
			"activitybar": [
				{
					"id": "swarm-account-manager",
					"title": "Swarm Account Manager",
					"icon": "media/SwarmDebugging.png"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}
